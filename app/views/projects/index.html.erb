
<%= link_to "New Project", new_project_path %>
<table>
    <thead>
        <tr>
            <th>Project Name</th>
            <th>Project Description</th>
        </tr>
    </thead>

    <tbody>
        <% @projects.each do |project| %>
            <tr>
                <td><%= project.name %></td>
                <td><%= project.description %></td>
            </tr>
        <% end %>
    </tbody>
</table>
